<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.tb_board.mapper.BoardMapper">

    <select id="getBoardList" resultType="com.example.tb_board.domain.Board">
        SELECT seq,
        title,
        content,
        reg_dt
        FROM tb_board
        ORDER BY seq desc;

    </select>

</mapper>